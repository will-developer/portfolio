generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Technology {
  id        Int     @id @default(autoincrement())
  nome      String
  descricao String
  imagem    String
  destaque  Boolean @default(false)
}
